{
  "name": "nuxt3-winston-log",
  "version": "1.0.0",
  "description": "nuxt3-winston-log",
  "keywords": [
    "nuxt",
    "nuxt3",
    "log",
    "winston",
    "nuxt3-winston-log"
  ],
  "authors": ["clark", "enjin"],
  "repository": "https://github.com/enjin-forks/nuxt3-winston-log.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && npm version minor && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "patch": "npm run lint && npm run test && npm run prepack && npm version patch && npm publish",
    "minor": "npm run lint && npm run test && npm run prepack && npm version minor && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@types/node": "^18.17.3",
    "changelogen": "^0.5.4",
    "eslint": "^8.46.0",
    "nuxt": "^3.6.5",
    "vitest": "^0.34.1"
  }
}
